<!--components/power/power.wxml-->
<view class="power">
  <view class="top">
    <view class="left">音乐新势力</view>
    <view class="right">新音乐</view>
  </view>
  <view class="result">
    <view wx:for="{{result}}" wx:key="{{key}}" class="item">
      <view class="box">
        <image src="{{item.song.album.picUrl}}" class="img"></image>
      </view>
      <view class="text">{{item.name}}</view>
    </view>
  </view>
</view>
